<h1>Feed</h1>

<p>
  <strong>Title:</strong>
  <%=h @feed.title %>
</p>
<p>
  <strong>Author:</strong>
  <%=h @feed.author %>
</p>
<p>
  <strong>Content:</strong>
  <%=h @feed.content %>
</p>

<p>
  <%= link_to "Edit", edit_feed_path(@feed) %> |
  <%= link_to "Destroy", @feed, :confirm => 'Are you sure?', :method => :delete %> |
  <%= link_to "View All", feeds_path %>
</p>
